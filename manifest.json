{
  "manifest_version": 3,
  "name": "LocalStorage Labyrinth",
  "version": "1.0",
  "description": "一个安全的LocalStorage数据加密管理插件",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "image_fx.jpg",
      "48": "image_fx.jpg",
      "128": "image_fx.jpg"
    }
  },
  "icons": {
    "16": "image_fx.jpg",
    "48": "image_fx.jpg",
    "128": "image_fx.jpg"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }]
}